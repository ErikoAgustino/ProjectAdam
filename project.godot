[application]

config/name="ProjectAdam"
run/main_scene="res://scene/ui/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

PlayerStatus="*res://script/PlayerStatus.gd"
PlayerInventory="*res://script/PlayerInventory.gd"
JsonData="*res://script/JSONData.gd"
GameManager="*res://script/GameManager.gd"
SoundManager="*res://addons/sound_manager/module/SoundManager.tscn"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/sound_manager/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"detect_3d": false,
"flags/filter": false
}

